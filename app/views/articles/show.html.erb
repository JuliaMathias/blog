<p id="notice"><%= notice %></p>
<div class="mb-3">
  <% @article.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 200, crop: :fill %>
<% end %>
</div>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @article.body %>
</p>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', articles_path %>
